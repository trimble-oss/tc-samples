<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SignIn.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SignIn.Maui"
    Shell.FlyoutBehavior="Disabled"
    Shell.NavBarIsVisible="False"
    Title="SignIn.Maui">

    <ShellContent ContentTemplate="{DataTemplate local:LoginView}" Route="LoginView" />
    <ShellContent ContentTemplate="{DataTemplate local:ProjectsView}" Route="ProjectsView" />

</Shell>
