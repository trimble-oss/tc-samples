{
  "_comment": "Mend (WhiteSource) Configuration File for Trimble Connect .NET SDK Samples",
  
  "apiKey": "{env.MEND_API_KEY}",
  "url": "https://saas.whitesourcesoftware.com/agent",
  
  "productName": "Trimble Connect",
  "projectName": "tc-samples-{0}",
  "productVersion": "1.0", 
  "appName": "Connect DevOps",
  
  "includes": [
    "**/*.dll",
    "**/*.exe", 
    "**/*.nupkg",
    "**/*.jar"
  ],
  "excludes": [
    "**/node_modules/**",
    "**/bin/Debug/**",
    "**/bin/Release/**",
    "**/obj/**",
    "**/.git/**",
    "**/packages/**",
    "**/.vs/**",
    "**/TestResults/**"
  ],
  
  "dotNet": {
    "enableSourceCodeScanning": false,
    "resolveDependencies": true,
    "collectDependenciesAtRuntime": false,
    "ignoreSourceFiles": true
  },
  
  "nuget": {
    "resolveDependencies": true,
    "restoreDependencies": true,
    "ignoreSourceFiles": true
  },
  
  "checkPolicies": true,
  "forceCheckAllDependencies": false,
  "forceUpdate": false,
  "offline": false,
  
  "archiveExtractionDepth": 2,
  "archiveIncludes": [
    "**/*.war",
    "**/*.ear", 
    "**/*.zip",
    "**/*.tar.gz",
    "**/*.tgz",
    "**/*.tar",
    "**/*.car"
  ],
  "archiveExcludes": [
    "**/*sources.jar",
    "**/*javadoc.jar"
  ],
  
  "updateInventory": true,
  "resolveAllDependencies": false,
  "logLevel": "info",
  
  "requesterEmail": "{env.MEND_EMAIL}"
}