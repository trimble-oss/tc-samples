#######################################
# Mend (WhiteSource) Configuration File
# For Trimble Connect .NET SDK Samples
#######################################

# API Configuration
apiKey={env.MEND_API_KEY}
wss.url=https://saas.whitesourcesoftware.com/agent

# Product and Project Configuration
productName=Trimble Connect
projectName=tc-samples-{0}
productVersion=1.0

# Project Token (optional - will be generated if not provided)
# projectToken=

# Include/Exclude Patterns
includes=**/*.dll **/*.exe **/*.nupkg **/*.jar
excludes=**/node_modules/** **/bin/Debug/** **/bin/Release/** **/obj/** **/.git/** **/packages/** **/.vs/** **/TestResults/**

# .NET Specific Configuration
dotNet.enableSourceCodeScanning=false
dotNet.resolveDependencies=true
dotNet.collectDependenciesAtRuntime=false
dotNet.ignoreSourceFiles=true

# NuGet Configuration
nuget.resolveDependencies=true
nuget.restoreDependencies=true
nuget.preferredEnvironment=
nuget.packagesDirectory=
nuget.ignoreSourceFiles=true

# Scan Configuration
checkPolicies=true
forceCheckAllDependencies=false
forceUpdate=false
offline=false

# Archive Settings
archiveExtractionDepth=2
archiveIncludes=**/*.war **/*.ear **/*.zip **/*.tar.gz **/*.tgz **/*.tar **/*.car
archiveExcludes=**/*sources.jar **/*javadoc.jar

# General Settings
updateInventory=true
resolveAllDependencies=false
logLevel=info

# Proxy Settings (uncomment if needed)
# proxy.host=
# proxy.port=
# proxy.user=
# proxy.pass=

# Request Settings
requesterEmail=