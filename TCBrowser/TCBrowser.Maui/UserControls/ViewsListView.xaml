<?xml version="1.0" encoding="utf-8" ?>
<ContentView 
    x:Class="TCBrowser.Maui.UserControls.ViewsListView"
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:controls="clr-namespace:TCBrowser.Maui.UserControls"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             xmlns:viewmodels="clr-namespace:TCBrowser.Maui.ViewModels"
             x:Name="ViewsListview"
             x:DataType="viewmodels:ProjectVm">
    <CollectionView ItemsSource="{Binding ViewsList}">
        <CollectionView.ItemTemplate>
            <DataTemplate>
                <Frame Margin="5" Padding="10">
                    <Label Text="{Binding .}" />
                </Frame>
            </DataTemplate>
        </CollectionView.ItemTemplate>
    </CollectionView>
</ContentView>
