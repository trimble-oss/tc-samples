<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TCBrowser.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TCBrowser.Maui"
    Shell.FlyoutBehavior="Disabled"
    Shell.NavBarIsVisible="False"
    Title="TCBrowser.Maui">

    <ShellContent ContentTemplate="{DataTemplate local:LoginView}" Route="LoginView" />
    <ShellContent ContentTemplate="{DataTemplate local:ProjectsView}" Route="ProjectsView" />
    <ShellContent ContentTemplate="{DataTemplate local:ProjectDetailsView}" Route="ProjectDetailsView" />
</Shell>
