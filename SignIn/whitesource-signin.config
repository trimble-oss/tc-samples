{
  "_comment": "Mend (WhiteSource) Configuration File for Trimble Connect SignIn Solution",
  
  "apiKey": "{env.WS_APIKEY}",
  "userKey": "{env.WS_SERVICEUSER}",
  "url": "https://saas.whitesourcesoftware.com/agent",
  "wss.url": "https://saas.whitesourcesoftware.com/agent",
  
  "productName": "Trimble Connect",
  "projectName": "SignIn",
  "productVersion": "1.0", 
  "appName": "Connect DevOps",
  "requesterEmail": "{env.WS_SERVICEUSER}",
  
  "includes": [
    "**/*.dll",
    "**/*.exe", 
    "**/*.nupkg",
    "**/*.jar",
    "**/*.csproj",
    "**/*.sln",
    "**/packages.config"
  ],
  "excludes": [
    "**/node_modules/**",
    "**/bin/Debug/**",
    "**/bin/Release/**",
    "**/obj/**",
    "**/.git/**",
    "**/packages/**",
    "**/.vs/**",
    "**/TestResults/**",
    "**/*Test*/**",
    "**/*test*/**"
  ],
  
  "dotNet": {
    "enableSourceCodeScanning": false,
    "resolveDependencies": true,
    "collectDependenciesAtRuntime": false,
    "ignoreSourceFiles": true
  },
  
  "nuget": {
    "resolveDependencies": true,
    "restoreDependencies": true,
    "ignoreSourceFiles": true
  },
  
  "checkPolicies": true,
  "forceCheckAllDependencies": false,
  "forceUpdate": false,
  "offline": false,
  "generateProjectDetailsJson": true,
  "enableLicenseViolations": true,
  
  "archiveExtractionDepth": 2,
  "archiveIncludes": [
    "**/*.war",
    "**/*.ear", 
    "**/*.zip",
    "**/*.tar.gz",
    "**/*.tgz",
    "**/*.tar",
    "**/*.car"
  ],
  "archiveExcludes": [
    "**/*sources.jar",
    "**/*javadoc.jar"
  ],
  
  "updateInventory": true,
  "resolveAllDependencies": false,
  "logLevel": "info",
  "scanComment": "Automated scan from GitHub Actions - SignIn Solution"
}